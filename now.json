{
  "version": 2,
  "alias": ["trends.now.sh"],
  "env": {
    "GITHUB_ACCESS_TOKEN": "@github-access-token"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.tsx",
      "headers": {
        "cache-control": "public, s-maxage=43200, max-age=43200, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/www/$1",
      "headers": {
        "cache-control": "public, s-maxage=43200, max-age=43200, immutable",
        "Service-Worker-Allowed": "/"
      }
    }
  ],
  "builds": [
    {
      "src": "www/next.config.js",
      "use": "@now/next"
    },
    {
      "src": "api/index.tsx",
      "use": "@now/node-server@canary"
    },
    {
      "src": "www/static/*",
      "use": "@now/static"
    }
  ]
}
