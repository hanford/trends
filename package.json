    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/next": "^9.0.0",
    "@types/node": "^18.11.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "husky": "^1.2.1",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0",
    "typescript": "^4.7.4"
  },
  "husky": {
    "hooks": {
